<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="todo" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mlanches.UI.AbrirMesaController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Cliente: *" />
        <TextField fx:id="txt_cliente" layoutX="14.0" layoutY="31.0" prefHeight="25.0" prefWidth="323.0" />
        <Button layoutX="344.0" layoutY="31.0" mnemonicParsing="false" onAction="#evt_procurar" text="Pesquisar" />
        <TableView fx:id="tabela" layoutX="14.0" layoutY="66.0" prefHeight="220.0" prefWidth="392.0">
            <columns>
                <TableColumn fx:id="tc_nome" prefWidth="153.0" text="Nome" />
                <TableColumn fx:id="tc_endereco" prefWidth="145.0" text="Endereco" />
                <TableColumn fx:id="tc_saldoDevedor" prefWidth="93.0" text="Saldo devedor" />
            </columns>
        </TableView>
        <Label layoutX="24.0" layoutY="295.0" text="Numero da mesa: *" />
        <Separator layoutX="14.0" layoutY="347.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
        <Button layoutX="189.0" layoutY="360.0" mnemonicParsing="false" onAction="#evt_confirmar" prefHeight="25.0" prefWidth="104.0" text="Confirmar" />
        <Button layoutX="300.0" layoutY="360.0" mnemonicParsing="false" onAction="#evt_cancelar" prefHeight="25.0" prefWidth="104.0" text="Cancelar" />
        <TextField fx:id="txt_nmesa" layoutX="24.0" layoutY="312.0" prefHeight="25.0" prefWidth="94.0" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</AnchorPane>
