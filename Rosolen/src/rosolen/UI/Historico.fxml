<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rosolen.UI.HistoricoController">
    <children>
        <BorderPane fx:id="pn_historico">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <top>
                <TableView fx:id="tabelaHistorico" prefHeight="378.0" prefWidth="930.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn fx:id="col_data" prefWidth="135.0" text="Data da Mensalidade" />
                        <TableColumn fx:id="col_dataPagamento" prefWidth="134.0" text="Data do Pagamento" />
                        <TableColumn fx:id="col_descricao" prefWidth="379.0" text="Descrição" />
                        <TableColumn fx:id="col_valor" prefWidth="97.0" text="Valor Pago" />
                        <TableColumn fx:id="col_funcionario" prefWidth="184.0" text="Funcionario" />
                    </columns>
                </TableView>
            </top>
        </BorderPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</VBox>
